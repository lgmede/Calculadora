<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from CalculadoraService WSDL service -->

  <service name="Calculadora" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://prueba.com"/>
      <parameter name="wsdlServiceElement" value="CalculadoraService"/>
      <parameter name="schemaQualified" value="http://prueba.com"/>
      <parameter name="wsdlServicePort" value="Calculadora"/>
      <parameter name="className" value="com.prueba.CalculadoraSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Calculadora"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="suma" qname="operNS:suma" xmlns:operNS="http://prueba.com" returnQName="retNS:sumaReturn" xmlns:retNS="http://prueba.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:num1" xmlns:pns="http://prueba.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:num2" xmlns:pns="http://prueba.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="suma"/>

  </service>
</deployment>
